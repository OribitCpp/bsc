// RUN: %clang_cc1 -fsyntax-only -verify %s

trait F<T> {
  T foo(This* this, T a);
  T bar(This* this);
};

struct S{
  int x;
};

int struct S::foo(struct S* this, int a) {
  return this->x;
}

impl trait F<int> for struct S; // expected-error {{function 'bar' in 'trait F<int>' is not implemented for 'struct S'}}

