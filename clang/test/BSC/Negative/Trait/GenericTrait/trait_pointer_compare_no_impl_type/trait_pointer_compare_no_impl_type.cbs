// RUN: %clang_cc1 -fsyntax-only -verify %s

trait Future<T> {
    T poll(This* this);
};

int int::poll(int* this) {
    return *this;
}

impl trait Future<int> for int;

int main() {
    int d = 1;
    trait Future<int> *f = &d;
    if (f == 1.0) {  // expected-error {{expected a pointer type which has implemented 'trait Future<int>', found 'double'}}
        return 1;
    }
   
    return 0;
}
