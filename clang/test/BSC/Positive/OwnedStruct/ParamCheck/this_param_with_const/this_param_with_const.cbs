#include <stdlib.h>

int x = 0;
owned struct Foo {
  public:
    int a;
  ~Foo(Foo this){
   x++;
  }
};

int Foo::getA(const Foo* this) {
  return this->a;
}

int goo() {
  Foo foo ={.a = 42};
  if(foo.getA()!=42){
    return 1;
  };
  return 0;
}

int main() {
  goo();
  return x-1;
}