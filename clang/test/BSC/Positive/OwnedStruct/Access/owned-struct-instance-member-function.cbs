// RUN: %test.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output

owned struct A {
    public:
        int a;
        int f(A* this) {return this->a; }
};

int main() {
    A a = {.a = 1};
    int b = A::f(&a);
    int c = a.f();
    return b - c;
}
