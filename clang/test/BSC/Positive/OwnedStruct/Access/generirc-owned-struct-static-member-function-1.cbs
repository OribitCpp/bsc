owned struct A<T> {
public:
    T z;
    T foo(T a) {
        return a;
    }
};

// CHECK-L: #line 13 "{{.*}}.cbs"
int main() {
    char y = 's';
    A<char> aaa = {y};
    A<char>::foo(1);
    return 0;
}


