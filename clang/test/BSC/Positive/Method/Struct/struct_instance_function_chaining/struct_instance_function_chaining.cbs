// RUN: %clang_cc1 -verify %s
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

struct queue {
};

void struct queue::isEmpty(struct queue* this){}

struct Scheduler {
    struct queue* que;

};

void struct Scheduler::run(struct Scheduler* this) {
      this->que->isEmpty();
      (*this->que).isEmpty();
}

int main() {
    struct Scheduler s;
    s.run();
    return 0;
}

