// RUN: %clang_cc1 -verify %s
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

void int::increase(int* this) {
    *this = *this +1;
}

int main() {
  int foo = 1; 
  void (*fp1)(int *) = int::increase;
  void (*fp2)(int *) = &int::increase;
  fp1(&foo);
  fp2(&foo);
  return 0;
}

// CHECK: void int_increase(int *this) {
// CHECK-NEXT:     *this = *this + 1;
// CHECK-NEXT: }

// CHECK: int main(void) {
// CHECK-NEXT:   int foo = 1;
// CHECK-NEXT:   void (*fp1)(int *) = int_increase;
// CHECK-NEXT:   void (*fp2)(int *) = &int_increase;
// CHECK-NEXT:   fp1(&foo);
// CHECK-NEXT:   fp2(&foo);
// CHECK-NEXT:   return 0;
// CHECK-NEXT: }