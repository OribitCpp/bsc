// RUN: %clang_cc1 -verify %s
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang_cc1 %t-rw.c -o %t-rw.output
// expected-no-diagnostics

typedef int bscInt;

void bscInt::increase_1(bscInt* this) {
  *this = *this + 1;
}

void int::increase_2(bscInt* this) {
  *this = *this + 1;
}

void int::increase_3(int* this) {
  *this = *this + 1;
}

// CHECK: typedef int bscInt;

// CHECK: void bscInt_increase_1(bscInt *this) {
// CHECK-NEXT:   *this = *this + 1;
// CHECK-NEXT: }

// CHECK: void int_increase_2(bscInt *this) {
// CHECK-NEXT:   *this = *this + 1;
// CHECK-NEXT: }

// CHECK: void int_increase_3(int *this) {
// CHECK-NEXT:   *this = *this + 1;
// CHECK-NEXT: }

