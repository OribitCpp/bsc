// RUN: %t-rw.output
// expected-no-diagnostics

#include <stdio.h>

void int::increase(int* this) {
    *this = *this +1;
}


int main() {
  int x = 1;
  int* y = &x;
  y->increase();
  y->increase();
  printf("%d", *y);
  printf("%d", x);
  return *y - 3;
}

