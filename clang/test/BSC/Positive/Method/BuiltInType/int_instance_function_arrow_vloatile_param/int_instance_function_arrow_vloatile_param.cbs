// expected-no-diagnostics

#include <stdio.h>

void int::increase(volatile int* this) {
    return;
}


int main() {
  int x = 1;
  int* y = &x;
  y->increase();
  y->increase();
  printf("%d", *y);
  printf("%d", x);
  return *y - 1;
}


