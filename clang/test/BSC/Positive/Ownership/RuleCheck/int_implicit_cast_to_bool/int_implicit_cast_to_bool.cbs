// RUN: %t-rw.output
// expected-no-diagnostics

#include <stdlib.h>

owned struct Vec<T> {
public:
    T* ptr;
    int len;
    ~Vec(Vec<T> this) {
        if (!this.ptr) free(this.ptr);
    }
};

safe Vec<T> Vec<T>::new(void) {
	unsafe {
		Vec<T> v = { .ptr = NULL, .len = 0 };
		return v;
	}
}

safe int Vec<T>::is_empty(const Vec<T>* borrow this) {
    return this->len == 0;
}

int main() {
    Vec<int> v = Vec<int>::new();
    _Bool flag = v.is_empty();
    return 0;
}

