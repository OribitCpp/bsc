// expected-no-diagnostics

#include "future.hbs"
#include <stdlib.h>

async int read(int a) {
  return 0;
}

async int f(int start) {
  int result = await read(await read(start));
  return result;
}

int main() {
  trait Future<int>* this = f(1);
  this->poll();
  this->free();
  return 0;
}