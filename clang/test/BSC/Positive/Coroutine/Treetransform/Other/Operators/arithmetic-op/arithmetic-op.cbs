#include "future.hbs"
#include <stdlib.h>

async int read(int a) {
  return 0;
}

async int f(int start) {
  int x = 3;
  int y = 5;
  int c;
  c = x + y;
  c = x - y;
  c = x * y;
  c = x / y;
  c = x % y;
  c = y ++;
  c = y --;
  int result = await read(1);
  return result;
}

int main() {
 trait Future<int>* this = f(1);
  this->poll();
  return 0;
}