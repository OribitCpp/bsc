#include "future.hbs"
#include <stdlib.h>

async int read(int a) {
  return 0;
}

int max(int x, int y)
{
    return x > y ? x : y;
}

async int f(int start) {
  int (* p)(int, int) = & max;
  int a = 1, b = 2, c = 3, d;

  d = p(p(a, b), c);

  int result = await read(1);
  return d;
}

int main() {
  trait Future<int>* this = f(1);
  this->poll();
  return 0;
}