// expected-no-diagnostics

#include "future.hbs"
#include <stdlib.h>

async int read(int a) {
  return 0;
}

async int f(int start) {
    long myLong = 100000L;
    unsigned int myUnsignedInt = 10U;
    float myFloat = 3.14f;
    double myDouble = 3.14159;
    int result = await read(2);
    return result + 5 + start;
}

int main() {
  trait Future<int>* this = f(1);
  this->poll();
  return 0;
}