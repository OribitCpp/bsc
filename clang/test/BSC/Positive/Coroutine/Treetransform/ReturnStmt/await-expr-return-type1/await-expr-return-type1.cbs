#include "future.hbs"
#include <stdlib.h>

async int read(int a) {
  return 0;
}

async int f(int start) {
    int result = await read(1);
    return start * result;
}

async int g() {
    int result = await f(2);
    return result + 5;
}

int main() {
    g();
    return 0;
}
