// expected-no-diagnostics

#include "future.hbs"
#include <stdlib.h>

async int read(int a) {
  return 0;
}

typedef unsigned int UINT32;
async UINT32 f() {
    await read(1);
    return 0;
}

trait Future<unsigned int>* f_wrapper() {
    trait Future<unsigned int>* a = f();
    return a;
}

trait Future<unsigned int>* (*CallBack)() = f_wrapper;
async UINT32 g() {
    UINT32 i = 0;
    i = await CallBack();
    return i;
}

async unsigned int q() {
    unsigned int i = 0;
    i = await CallBack();
    return i;
}

async UINT32 h() {
    UINT32 i = 0;
    i = await q();
    return i;
}

int main() {
    trait Future<unsigned int>* this = g();
    this->poll();
    this->free();
    return 0;
}