// RUN: %clang_cc1 -emit-llvm -o - %s | FileCheck %s
// RUN: %clang %s -g -o %test.output
// RUN: %test.output
// expected-no-diagnostics

// CHECK-DAG: define {{.*}} @add_int
T add<T>(T a, T b) {
    return a + b;
}

// CHECK-DAG: define {{.*}} @max_double
T max<T>(T a, T b) {
  return a > b ? a : b;
}

int main() {
    int sum = add(1, 2);
    double r = max<double>(2.5, 2.6);
}
