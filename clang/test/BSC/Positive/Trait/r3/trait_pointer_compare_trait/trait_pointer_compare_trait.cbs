// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

trait F {
  int foo(This* this);
};

int int::foo(int *this) {
  return 2;
};

impl trait F for int;

int traitPtrEqualTraitPtr() {
  int a = 1;
  trait F* f = &a;
  trait F* e = &a;
  if (f != e) {
    return 1;
  }
  if (f == e) {
    return 0;
  }
  if ((trait F*)e != (trait F*)f) {
    return 1;
  }
  return 1;
}

int traitPtrNotEqualTraitPtr() {
  int a = 1;
  int b = 1;
  trait F* f = &a;
  trait F* e = &b;
  if (f == e) {
    return 1;
  }
  if (f != e) {
    return 0;
  }
  if ((trait F*)e != (trait F*)f) {
    return 0;
  }
  return 1;
}

int main() {
  if (traitPtrEqualTraitPtr() || traitPtrNotEqualTraitPtr()) {
    return 1;
  }
  return 0;
}

