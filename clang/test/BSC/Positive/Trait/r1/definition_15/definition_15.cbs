// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// RUN: %clang -rewrite-bsc -line %s -o %t-rwline.c
// RUN: FileCheck --input-file=%t-rwline.c %s --check-prefix=CHECK
// RUN: FileCheck --input-file=%t-rwline.c %s --check-prefix=CHECK-L
// expected-no-diagnostics

trait F {
  int foo(This* this);
};

// CHECK-L: #line 17 "{{.*}}.cbs"
trait F* get(trait F* f) {
  return f;
};

// CHECK-L: #line 22 "{{.*}}.cbs"
int int::foo(int* this) {
  return 0;
}

impl trait F for int;

// CHECK-L: #line 29 "{{.*}}.cbs"
int main() {
  int a = 1;
  trait F* f = get(&a);
  return 0;
}

// CHECK:      struct __Trait_F_Vtable {
// CHECK-NEXT:   int (*foo)(void *);
// CHECK-NEXT: };

// CHECK:      struct __Trait_F {
// CHECK-NEXT:   void *data;
// CHECK-NEXT:   struct __Trait_F_Vtable *vtable;
// CHECK-NEXT: };

// CHECK:      struct __Trait_F get(struct __Trait_F f) {
// CHECK-NEXT:   return f;
// CHECK-NEXT: }

// CHECK:      int int_foo(int *this) {
// CHECK-NEXT:   return 0;
// CHECK-NEXT: }

// CHECK:      struct __Trait_F_Vtable __int_trait_F = {.foo = (int (*)(void *))int_foo};

// CHECK:      int main(void) {
// CHECK-NEXT:   int a = 1;
// CHECK-NEXT:   struct __Trait_F f = get((struct __Trait_F){.data = &a, .vtable = &__int_trait_F});
// CHECK-NEXT:   return 0;
// CHECK-NEXT: }