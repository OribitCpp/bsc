// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

#include <stdio.h>


trait Future<T> {
    T poll(This* this);
};

int main() {
    trait Future<int> *f = NULL;
    if (f != NULL) {
        return 1;
    }
    if (f == NULL) {
        return 0;
    }
    if (NULL == (trait Future<int> *)f) {
        return 0;
    }
    return 1;
}

// CHECK:      struct __Trait_Future_int {
// CHECK-NEXT:   void *data;
// CHECK-NEXT:   struct __Trait_Future_Vtable_int *vtable;
// CHECK-NEXT: };
// CHECK-EMPTY:
// CHECK-NEXT: struct __Trait_Future_Vtable_int {
// CHECK-NEXT:   int (*poll)(void *);
// CHECK-NEXT: };
// CHECK-EMPTY:
// CHECK-NEXT: int main(void) {
// CHECK-NEXT:   struct __Trait_Future_int f = {((void *)0), ((void *)0)};
// CHECK-NEXT:   if (f.data != ((void *)0)) {
// CHECK-NEXT:     return 1;
// CHECK-NEXT:   }
// CHECK-NEXT:   if (f.data == ((void *)0)) {
// CHECK-NEXT:     return 0;
// CHECK-NEXT:   }
// CHECK-NEXT:   if (((void *)0) == f.data) {
// CHECK-NEXT:     return 0;
// CHECK-NEXT:   }
// CHECK-NEXT:   return 1;
// CHECK-NEXT: }