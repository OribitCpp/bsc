// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

struct F<T> {
 T ele; 
};

struct E<T> {
  F<T> h;
};

int main(){
  struct F<int> f = {1};
  struct E<int> g = {f};

  struct F<struct E<int>> h = {g};
  struct E<struct E<int>> i = {h};
}

// CHECK:      struct F_int {
// CHECK-NEXT:   int ele;
// CHECK-NEXT: };
// CHECK-EMPTY:
// CHECK-NEXT: struct E_int {
// CHECK-NEXT:   struct F_int h;
// CHECK-NEXT: };
// CHECK-EMPTY:
// CHECK-NEXT: struct F_struct_E_int {
// CHECK-NEXT:   struct E_int ele;
// CHECK-NEXT: };
// CHECK-EMPTY:
// CHECK-NEXT: struct E_struct_E_int {
// CHECK-NEXT:   struct F_struct_E_int h;
// CHECK-NEXT: };
// CHECK-EMPTY:
// CHECK-NEXT: int main(void) {
// CHECK-NEXT:   struct F_int f = {1};
// CHECK-NEXT:   struct E_int g = {f};
// CHECK-NEXT:   struct F_struct_E_int h = {g};
// CHECK-NEXT:   struct E_struct_E_int i = {h};
// CHECK-NEXT: }
