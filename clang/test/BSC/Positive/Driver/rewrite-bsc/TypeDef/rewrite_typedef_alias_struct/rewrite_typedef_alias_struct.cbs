// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: FileCheck --input-file=%t-rw.c %s
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output

typedef MyLongInt = long int;
typedef long long int LI;

// typedef int[5] MyIntArray;


struct Animal<T, int N> {
    MyLongInt height;
    LI age;
    T weight;
};

typedef struct Animal<int, 10> MyAnimal;
typedef MyAnimal2 = struct Animal<int, 10>;

typedef Dog<T, int N> = struct Animal<T, N>;
typedef HuskyDog = Dog<MyLongInt, 10>;

void Animal<T, int N>::bar(Animal<T, int N> *this) {
    typedef double MyDouble;
    typedef Myfloat = float;
    MyDouble a = 1;
    Myfloat b = 2;
    MyDouble f = a + b;
    LI e;
}


int main() {
  typedef int* MyPointType;
  typedef MyPointType2 = int*;
  MyPointType p1;
  MyPointType2 p2;
  MyLongInt c;
  LI d = 1;
  LI e = 2;
  MyAnimal animal = {6};
  MyAnimal2 animal2 = {8};
  HuskyDog husky = {10};
  husky.bar();
}

