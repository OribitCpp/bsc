// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

typedef Uch = unsigned char;
struct S<T> {
    T a;
};
typedef MyS_T<T> = struct S<T>; 
int main() {
    MyS_T<Uch> b;
    b.a = 'p';
    if (b.a == 'p') {
        return 0;
    } else {
        return 1;
    }
}