// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

struct Array<T, int N> { 
    T a[N];
};

struct IntArray<int N> {
    int a[N];
};

struct Pair<K, V> {
   K a;
   V b;
};

typedef Pair_IntArray<T, int N> = struct Pair<T, struct IntArray<N>>;
typedef Pair_Array<T1, T2, int N> = struct Pair<T1, struct Array<T2, N>>;

int main() {
    IntArray<2> intarr = { .a = { 5, 6 } };
    Pair_IntArray<int, 2> p1 = { .a = 5, .b = intarr };

    Array<float, 2> floatarr = { .a = { 0.5, 0.6 } };
    Pair_Array<int, float, 2> p2 = { .a = 5, .b = floatarr };

    int i = p1.b.a[1];
    float f = p2.b.a[1];
    
    return 0;
}