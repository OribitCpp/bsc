#include "bsc_conditional.hbs"
struct S {
    int x;
};

constexpr int g_a = 0;

int foo(int a) {
    conditional<g_a, struct S, int> b = a;
    return b;
}

int main() {
    int a = {1};
    int b = foo(a);
    if (a == b) {
        return 0;
    }
    return 1;
}