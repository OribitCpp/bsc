// expected-no-diagnostics

struct PollResult<T> {
    _Bool isPending;
};

struct PollResult<int> int::poll() {
    struct PollResult<int> s;
    return s;
}

void struct PollResult<T>::foo(struct PollResult<T> *this) {
}


int main() {
    struct PollResult<int> (*pollFunc)() = int::poll;
    struct PollResult<int> res = pollFunc();
    if (res.isPending) {
        res.foo();
    }
    return 0;
}
