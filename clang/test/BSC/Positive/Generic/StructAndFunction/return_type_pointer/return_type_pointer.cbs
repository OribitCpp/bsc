// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics


#include <stdio.h>


int * Foo<T> (T a, T b)  // pointer
{
  T x = a > b ? a : b;
  static int res = 42;
  return &res;
}


int * Bar<T> (T a, T b)  // array
{
  T x = a > b ? a : b;
  static int array[] = {0, 1, 2};
  return array;
}


int main ()
{
  int *res = Foo<int>(1, 2);
  printf("%d\n", *res);

  int *array = Bar<int>(1, 2);
  printf("%d %d %d\n", array[0], array[1], array[2]);

  return 0;
}
