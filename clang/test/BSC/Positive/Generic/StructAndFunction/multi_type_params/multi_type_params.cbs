// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

#include <stdio.h>
T2 foo<T1, T2, T3>(T1 a, T2 b, T3 c) {
  return b;
}

int main() {
  int f1 = foo<int, int, int>(1, 2, 3); 
  float f2 = foo<int, float, int>(1, 2.0, 3); 
  const char* c = "123";
  const char* f3 = foo<int, const char*, int>(2, c, 3);
  printf("f1 is %d\n", f1);
  printf("f2 is %f\n", f2);
  printf("f3 is %s\n", f3);
  return 0;
}
