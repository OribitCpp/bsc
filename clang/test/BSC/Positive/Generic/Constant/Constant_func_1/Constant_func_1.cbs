// RUN: %clang %s -o %t.output
// RUN: %t.output
// RUN: %clang -rewrite-bsc %s -o %t-rw.c
// RUN: %clang %t-rw.c -o %t-rw.output
// RUN: %t-rw.output
// expected-no-diagnostics

T constant_1<T, int B>(T a)
{
  T data[B];
  return a;
}

T constant_2<T, unsigned B>(T a)
{
  T data[B];
  return a;
}

T constant_3<T, signed B>(T a)
{
  T data[B];
  return a;
}

T constant_4<T, short B>(T a)
{
  T data[B];
  return a;
}

T constant_5<T, long B>(T a)
{
  T data[B];
  return a;
}

T constant_6<T, long long B>(T a)
{
  T data[B];
  return a;
}

int main() {
  int a1 = constant_1<int, 5>(1);
  int a2 = constant_2<int, 5>(1);
  int a3 = constant_3<int, 5>(1);
  int a4 = constant_4<int, 5>(1);
  int a5 = constant_5<int, 5>(1);
  int a6 = constant_6<int, 5>(1);
  
  return 0;
}