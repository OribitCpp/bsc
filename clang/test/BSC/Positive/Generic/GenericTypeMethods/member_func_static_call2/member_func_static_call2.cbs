// expected-no-diagnostics

struct S<T> {
};

void struct S<T>::foo(T *uf) {
}

void struct S<T>::bar(T * uf) {
}

void struct S<T>::getA(T *uf) {
  int a = 10;
  a > 10? struct S<T>::foo(uf) : struct S<T>::bar(uf);
}

int main() {
    int a = 10;
    struct S<int>::getA(&a);
    return 0;
}
